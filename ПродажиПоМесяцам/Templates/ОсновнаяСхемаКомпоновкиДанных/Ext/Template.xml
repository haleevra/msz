		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Выручка.ПериодДень КАК День,
	ВЫБОР
		КОГДА ЮридическийАдрес.Город = ""
			ТОГДА ЮридическийАдрес.Регион
		ИНАЧЕ ЮридическийАдрес.Город
	КОНЕЦ КАК Город,
	КонтрагентыМенеджер.Значение КАК Менеджер,
	Выручка.КорСубконто1 КАК Контрагент,
	Выручка.Субконто1 КАК Номенклатура,
	Выручка.Субконто1.ЕдОтпуска.Вес КАК Фасовка,
	СУММА(Количество.КоличествоОборотКт * Выручка.Субконто1.ЕдОтпуска.Вес) КАК Количество,
	СУММА(Выручка.СуммаОборотКт) КАК Сумма,
	Выручка.Регистратор КАК Регистратор,
	Выручка.ПериодСекунда КАК Секунда,
	Выручка.ПериодМесяц КАК Месяц,
	Выручка.ПериодКвартал КАК Квартал,
	Выручка.ПериодПолугодие КАК Полугодие,
	Выручка.ПериодГод КАК Год,
	ВЫРАЗИТЬ(Выручка.Субконто1.НаименованиеПолное КАК СТРОКА(1000)) КАК НоменклатураПредставление
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(, , Авто, Счет В ИЕРАРХИИ (&amp;СчетаВыручки), &amp;Номенклатура, , , &amp;Контрагенты) КАК Выручка
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.Обороты(, , Авто, Счет = &amp;ГотоваяПродукция, &amp;Номенклатура, Субконто1.ВидНоменклатуры.Наименование = "Продукция", , ) КАК Количество
		ПО Выручка.Регистратор = Количество.Регистратор
			И Выручка.Субконто1 = Количество.Субконто1
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК ЮридическийАдрес
		ПО Выручка.КорСубконто1 = ЮридическийАдрес.Ссылка
			И (ЮридическийАдрес.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ЮрАдресКонтрагента))
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.ДополнительныеРеквизиты КАК КонтрагентыМенеджер
		ПО Выручка.КорСубконто1 = КонтрагентыМенеджер.Ссылка
			И (КонтрагентыМенеджер.Свойство.Заголовок = "Менеджер")
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Номенклатура.ДополнительныеРеквизиты КАК НоменклатураБренд
		ПО Выручка.Субконто1 = НоменклатураБренд.Ссылка
			И (НоменклатураБренд.Свойство.Заголовок = "Бренд")
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Номенклатура.ДополнительныеРеквизиты КАК НоменклатураСегмент
		ПО Выручка.Субконто1 = НоменклатураСегмент.Ссылка
			И (НоменклатураСегмент.Свойство.Заголовок = "Сегмент")

СГРУППИРОВАТЬ ПО
	ВЫБОР
		КОГДА ЮридическийАдрес.Город = ""
			ТОГДА ЮридическийАдрес.Регион
		ИНАЧЕ ЮридическийАдрес.Город
	КОНЕЦ,
	Выручка.ПериодДень,
	КонтрагентыМенеджер.Значение,
	Выручка.КорСубконто1,
	Выручка.Регистратор,
	Выручка.ПериодМесяц,
	Выручка.ПериодСекунда,
	Выручка.ПериодПолугодие,
	Выручка.ПериодГод,
	Выручка.ПериодКвартал,
	Выручка.Субконто1,
	Выручка.Субконто1.ЕдОтпуска.Вес,
	ВЫРАЗИТЬ(Выручка.Субконто1.НаименованиеПолное КАК СТРОКА(1000))</query>
	</dataSet>
